{{# partial "memberHeader"}}



    <div class="hc-content d-flex justify-content-center">
        <div class="hc-card d-flex justify-content-center w-50 flex-column align-items-center">

                <div>
                    <h5>Seleccione el ID del Trayecto al que se quiere sumar</h5>
                </div>

        <div class="d-flex justify-content-center w-50 align-items-center">
                <form method="post" class="w-100 d-flex justify-content-center">
                    <select name="trayecto" class="form-control form-control-inline mx-3 w-25">
                        <option value="" disabled selected> --- </option>
                        {{#each trayectos}}
                            <option value="{{id}}">{{id}}</option>
                        {{/each}}
                    </select>
                    <button type="submit" class="btn btn-primary btn-inline mx-3 w-25">Elegir</button>
                </form>
            </div>
        </div>
    </div>

{{/partial}}
{{> headers/memberHeader.hbs}}
